# Task 1: while loop
number = 0
while number <= 20:
    print(number)
    number+=2
    
# Task 2: for loop with continue 
for number in range(1, 16): 
    if number % 3 == 0: 
        continue  
    print(number)  
      
# Task 3: if-else statement
number = int(input("Enter a number: "))

if number >0:
    print('Number is positive')
elif number<0:
    print('number is negative')
else:
    print('number is equal to zero')
    

# Task 4 Nested loops
for i in range(1, 6):
    # Inner loop for multiplication from 1 to 5
    for j in range(1, 6):
        print(f"{i} x {j} = {i * j}", end="\t")  
    print() 
